digraph {
	"start (S0)"
	S2
	S3
	S6
	"end (S9)" [shape=doublecircle]
	"start (S0)" -> S2 [label=a]
	"start (S0)" -> S3 [label=b]
	S2 -> S2 [label=a]
	S2 -> S6 [label=b]
	S3 -> S2 [label=a]
	S3 -> S3 [label=b]
	S6 -> S2 [label=a]
	S6 -> "end (S9)" [label=b]
	"end (S9)" -> S2 [label=a]
	"end (S9)" -> S3 [label=b]
}
